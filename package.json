  "license": "MIT",
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --fix ./",
    "db-dev-push": "dotenv -e .env.development.local prisma db push",
    "db-dev-migrate": "dotenv -e .env.development.local prisma migrate dev",
    "db-dev-seed": "dotenv -e .env.development.local prisma db seed",
    "db-dev-reset": "dotenv -e .env.development.local prisma migrate reset",
    "db-dev-client": "dotenv -e .env.development.local prisma studio",
    "db-prod-migrate": "dotenv -e .env.production.local prisma migrate dev",
    "db-prod-push": "dotenv -e .env.production.local prisma db push",
    "db-prod-seed": "dotenv -e .env.production.local prisma db seed",
    "db-prod-reset": "dotenv -e .env.production.local prisma migrate reset",
    "db-prod-deploy": "dotenv -e .env.production.local prisma migrate deploy",
    "db-prod-client": "dotenv -e .env.production.local prisma studio",
    "db-generate": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.3.1",
    "@reduxjs/toolkit": "^1.9.7",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/react-query": "^5.0.5",
    "@tanstack/react-query-devtools": "^5.4.3",
    "@tanstack/react-query-next-experimental": "^5.5.0",
    "@types/node": "20.6.3",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.16",
    "axios": "^1.6.0",
    "contentlayer": "^0.3.4",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "8.49.0",
    "eslint-config-next": "13.5.2",
    "next": "13.5.2",
    "next-auth": "^4.24.5",
    "next-contentlayer": "^0.3.4",
    "pino": "^8.16.2",
    "postcss": "8.4.30",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.1.3",
    "react-toastify": "^9.1.3",
    "tailwindcss": "3.3.3"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.5.4",
    "@tanstack/eslint-plugin-query": "^5.0.5",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "prettier": "^3.0.3",
    "prisma": "^5.3.1",
    "ts-node": "^10.9.1",
    "typescript": "5.2.2"
  }
